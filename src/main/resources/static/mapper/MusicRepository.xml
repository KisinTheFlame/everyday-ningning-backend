<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="tech.kisin.everydayningning.dao.MusicRepository">
    <select id="getAllDescription" resultType="java.lang.String">
        SELECT description
        FROM music
    </select>
    <select id="getByKeywordInexactly" resultType="MusicPO">
        SELECT *
        FROM music
        WHERE description LIKE '%#{keyword}%'
    </select>
</mapper>
